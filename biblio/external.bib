@article{compass-mumega-large,
title = {Micromegas as a large microstrip detector for the COMPASS experiment},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
volume = {469},
number = {2},
pages = {133-146},
year = {2001},
issn = {0168-9002},
doi = {https://doi.org/10.1016/S0168-9002(01)00769-0},
url = {https://www.sciencedirect.com/science/article/pii/S0168900201007690},
author = {D Thers and Ph Abbon and J Ball and Y Bedfer and C Bernet and C Carasco and E Delagnes and D Durand and J.-C Faivre and H Fonvieille and A Giganon and F Kunne and J.-M.Le Goff and F Lehar and A Magnon and D Neyret and E Pasquetto and H Pereira and S Platchkov and E Poisson and Ph Rebourgeard},
keywords = {Tracking detectors, Spectrometers},
abstract = {Recent results on the gaseous microstrip detector Micromegas which will be used to track particles in the COMPASS experiment at CERN are presented. Developments concerning its mechanical and electrical design, associated readout electronics and gas mixture were carried out. Particular attention was paid to the discharge phenomenon which affects this type of microstrip detector. The adequacy of the options finally retained, especially the SFE16 readout and the use of a Ne–C2H6–CF4 gas mixture, was demonstrated in a set of beam tests performed on a 26×36cm2 prototype. Operating at a gain of ∼6400, full efficiency is reached along with a spatial resolution of ∼50μm and a timing accuracy of 8.5ns. Discharges are kept at a low rate, less than one per SPS spill in a COMPASS-like environment. Via a decoupling of the strips through individual capacitors their impact is greatly reduced. They generate a dead time on the full detector of ∼3ms, affecting marginally the detection efficiency given their rate. The probability of discharge, at a given value of efficiency, is found to decrease with the mean value of the gas mixture atomic number. In view of these results, the commissioning of Micromegas for COMPASS is foreseen in the near future.}
}

