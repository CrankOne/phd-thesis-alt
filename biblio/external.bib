  ISSN                     = {0168-9002},
  Keywords                 = {Synchrotron detector, High energy electron tagging, Hadron rejection},
  Owner                    = {crank},
  Timestamp                = {2019.10.09},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0168900217305727}
}

@Article{7347477,
  Title                    = {On Detection of Narrow Angle${\rm e}\!+\!{\rm e}- $Pairs From Dark Photon Decays},
  Author                   = {A. V. {Dermenev} and S. V. {Donskov} and S. N. {Gninenko} and S. B. {Kuleshov} and V. A. {Matveev} and V. V. {Myalkovskiy} and V. D. {Peshekhonov} and V. A. {Poliakov} and A. A. {Savenkov} and V. O. {Tikhomirov} and I. A. {Zhukov}},
  Journal                  = {IEEE Transactions on Nuclear Science},
  Year                     = {2015},

  Month                    = {12},
  Number                   = {6},
  Pages                    = {3283-3287},
  Volume                   = {62},

  Abstract                 = {A class of models of a “dark sector” of particles consider new very weak interaction between the ordinary and dark matter transmitted by U'(1) gauge bosons A' (dark photons) mixing with ordinary photons. If such A's exist, they could be searched for in a light-shining-through-a-wall experiment in a high energy electron beam from the CERN SPS. A proposed search project suggests detection of the e+e- pairs produced in the A'-> e+e - decays with a very small opening angle. Coordinate detectors based on the thin-wall drift tubes with a minimal material budget and a two-hit resolution for e+ and e- tracks separated by more than 0.5 mm are considered as an option for detecting such events.},
  Doi                      = {10.1109/TNS.2015.2499794},
  Keywords                 = {dark matter;drift chambers;thin-wall drift tubes;coordinate detectors;CERN SPS;high-energy electron beam;U(1) gauge bosons;dark photon decays;Electron tubes;Gas detectors;Dark energy;Electromagnetics;Radiation detectors;Spatial resolution;Dark matter;${\rm e}\!+\!{\rm e} - $pairs;gas detectors},
